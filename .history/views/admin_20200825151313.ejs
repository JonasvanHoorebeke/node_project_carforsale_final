<%- include('header.ejs') %>

<div class="intro">
</br>
<input type="text" id="Larecherche" onkeyup="Recherche()" placeholder="Recherchez une annonce...">
</br></br>

<p>Voici les véhicules en vente actuellement</p>
</div>
<div class="container" id="Lesskins">

 <!--h2><%= title %></h2>-->
</br></br>
  <!--<a href="/skin/add" class="btn btn-sm btn-primary">Mettre en vente un véhicule</a><br/>-->
  <table id="Lesskins2" class="table table-dark table-striped">
    <thead>
      <tr>
        <th>Marque</th>
        <th>Modèle</th>
        <th>Année</th>
        <th>Kilométrage</th>
        <th>Prix</th>
      </tr>
    </thead>
    <tbody>
        <% annonce.forEach(function(item){ %>
            <tr>
              <td><%= item.marque %></td>
              <td><%= item.modele %></td>
              <td><%= item.annee %></td>
              <td><%= item.kilometrage %></td>
              <td><%= item.prix %> €</td>
        
                <td>
                    <a href="/annonce/get/<%= item.idannonce %>" class="btn btn-sm btn-primary">Voir</a>
                    <a href="/annonce/edit/<%= item.idannonce %>" class="btn btn-sm btn-primary">Modifier</a>
                    <a href="/annonce/delete/<%= item.idannonce %>" class="btn btn-sm btn-danger">Supprimer</a>
                    <!--<a href="/cart/<%= item.id %>" class="btn btn-sm btn-success">Acheter</a>-->
                </td>
            </tr>

            <div class="bs-example">
                <div class="card" style="max-width: 500px;">
                    <div class="row no-gutters">
                        <div class="col-sm-5" style="background: #868e96;">
                            <img src="/examples/images/sample.svg" class="card-img-top h-100" alt="...">
                        </div>
                        <div class="col-sm-7">
                            <div class="card-body">
                                <h5 class="card-title">Alice Liddel</h5>
                                <p class="card-text">Alice is a freelance web designer and developer based in London. She is specialized in HTML5, CSS3, JavaScript, Bootstrap, etc.</p>
                                <a href="#" class="btn btn-primary stretched-link">View Profile</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        





        <% }); %>
    </tbody>
</div>
</br>



    <%- include('header.ejs') %>
  <div class="intro">
  </br>
  <input type="text" id="Larecherche" onkeyup="Recherche()" placeholder="Recherchez un skin..." aria-label="recherche" autofocus>
  </br>
  </br>
    <p>Voici les skins actuellement en vente</p>
  </div>
<div class="container" id="Annonce">

 <!--h2><%= title %></h2>-->
</br></br>
  <a href="/annonce/add" class="btn btn-sm btn-primary">Mettre en vente un véhicule</a><br/>
  <table id="Annonce2" class="table table-dark table-striped">
    <thead>
      <tr>
        <th>Nom du skin</th>
        <th>Jeu concerné</th>
        <th>Prix (euros)</th>
        <th>Catégorie</th>
        <th>Rareté</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
        <% annonce.forEach(function(item) { %>
            <tr>
                <td><%= item.marque %></td>
                <td><%= item.modele %></td>
                <td><%= item.prix %> euros</td>
                <td><%= item.kilometrage %></td>
                <td><%= item.description %></td>

                <td>
                    <a href="/annonce/get/<%= item.idannonce %>" class="btn btn-sm btn-primary">Voir</a>
                    <!-- <a href="delete/<%= item.id %>" class="btn btn-sm btn-danger">Acheter</a> pour l'instant ça supprime -->
                    <a href="/cart/<%= item.idannonce %>" class="btn btn-sm btn-success">Acheter</a>
                </td>
            </tr>
        <% }); %>

        
    </tbody>
</div>
</br>
<%- include('footer.ejs') %>